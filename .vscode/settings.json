{
    "editor.tabSize": 4,
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true,
        "**/.hg/store/**": true,
        "out/**": true,
        ".venv/**": true,
        ".cache/**": true,
        "submodules/**": true,
    },
    "workbench.colorCustomizations": {
