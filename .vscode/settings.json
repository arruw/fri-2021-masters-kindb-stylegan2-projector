        "activityBarBadge.background": "#90d18e",
        "activityBarBadge.foreground": "#15202b",
        "statusBar.background": "#b34a4d",
        "statusBar.foreground": "#e7e7e7",
        "statusBarItem.hoverBackground": "#c36d70",
        "titleBar.activeBackground": "#b34a4d",
        "titleBar.activeForeground": "#e7e7e7",
        "titleBar.inactiveBackground": "#b34a4d99",
        "titleBar.inactiveForeground": "#e7e7e799"
